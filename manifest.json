{
	"manifest_version": 3,
	"name": "tieba extension",
	"version": "0.0.1",
	"background": { "scripts": ["background.js"] },
	"page_action": {
		"default_icon": {
			"19": "ba_19.png",
			"38": "ba_38.png"
		},
		"default_title": "tieba extension", 
		"default_popup": "popup.html"
	},
	"permissions" : ["tabs","http://localhost/"],
	"content_scripts":[{
		"matches":["http://tieba.baidu.com/*"],
		"js":["jquery-2.0.0.min.js", "content.js"]
	}]
}
